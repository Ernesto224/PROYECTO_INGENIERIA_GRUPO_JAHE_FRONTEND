{
  "version": 2,
  "builds": [
    {
      "src": "dist/hotel-jade/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/hotel-jade/index.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/hotel-jade/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/dist/hotel-jade/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}